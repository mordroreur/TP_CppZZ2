#include <iostream> 
#include "Point.hpp"

int main(int, char**) {
  Point p;

  p.setX(2);
  p.setY(0);
  std::cout << p.getX() << "," << p.getY() << std::endl;

  p.deplacerVers(10, 12);
  std::cout << p.getX() << "," << p.getY() << std::endl;

  p.deplacerDe(5,5);
  std::cout << p.getX() << "," << p.getY() << std::endl;
  
  return 0;
}
